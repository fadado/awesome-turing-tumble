/* Challenge #1, #2, #7 */

digraph Challenge01 {
	// graph attributes
	label="Challenge #1"
	fontname=Helvetica
	labelloc=t

	// global attributes
	node [ // for all nodes
		shape=circle
		fontname=Helvetica
	]
	edge [ // for all edges
	]

	// nodes
	start [shape=point]
	B_top [ // attributes for the blue top entry
		label="B"
		fontcolor=blue
	]
	B_bot [ // attributes for the blue bottom exit
		label="B"
		fontcolor=blue
	]

	// edges
	{rank=same start B_top}
	start -> B_top -> B_bot
}

